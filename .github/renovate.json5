{
  automerge: true,
  extends: [
    'config:recommended',
    ':dependencyDashboard',
  ],
  packageRules: [
    {
      "matchPackageNames": [
        "*"
      ],
      "dependencyDashboardApproval": false
    },
    {
      matchManagers: ["cargo"],
      groupName: "cargo dependencies",
    }
  ],
  platformAutomerge: true,
  automergeType: 'pr',
  prHourlyLimit: 0,
  prConcurrentLimit: 0,
  branchConcurrentLimit: 0,
  rebaseWhen: 'behind-base-branch',
}

